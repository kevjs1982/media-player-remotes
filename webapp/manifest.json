{
  "manifest_version": 2,
  "name": "SSDP Discovery and Control",
  "short_name": "SSDP Discovery",
  "description": "Discover Networked Media Players",
  "version": "1",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
    "scripts": ["main.js"]
   }
  },
  "permissions": [
  "http://*/",
  "https://*/",
    {"socket": [
    "udp-bind",
    "udp-send-to"
    ]}
  ]
}
